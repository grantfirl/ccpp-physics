[ccpp-table-properties]
  name = tiedtke_prog_clouds_pre
  type = scheme
  dependencies = machine.F, ccpp_saturation.f90

########################################################################
[ccpp-arg-table]
  name = tiedtke_prog_clouds_pre_init
  type = scheme
[errmsg]
  standard_name = ccpp_error_message
  long_name = error message for error handling in CCPP
  units = none
  dimensions = ()
  type = character
  kind = len=*
  intent = out
[errflg]
  standard_name = ccpp_error_code
  long_name = error code for error handling in CCPP
  units = 1
  dimensions = ()
  type = integer
  intent = out

########################################################################
[ccpp-arg-table]
  name = tiedtke_prog_clouds_pre_run
  type = scheme
[idim]
  standard_name = horizontal_loop_extent
  long_name = horizontal loop extent
  units = count
  dimensions = ()
  type = integer
  intent = in
[kdim]
  standard_name = vertical_layer_dimension
  long_name = number of vertical levels
  units = count
  dimensions = ()
  type = integer
  intent = in
[pdf_org]
  standard_name = do_cloud_pdf_definition_from_input_water_fields
  long_name = flag to activate definition of cloud pdf from input water fields
  units = flag
  dimensions = ()
  type = logical
  intent = in
[pdfcld]
  standard_name = do_pdf_clouds
  long_name = flag to activate PDF clouds
  units = flag
  dimensions = ()
  type = logical
  intent = in
[super_ice_opt]
  standard_name = control_for_prognostic_cloud_supersaturation
  long_name = control for prognostic cloud supersaturation
  units = 1
  dimensions = ()
  type = integer
  intent = in
[hlv]
  standard_name = latent_heat_of_vaporization_of_water_at_0C
  long_name = latent heat of evaporation/sublimation
  units = J kg-1
  dimensions = ()
  type = real
  kind = kind_phys
  intent = in
[hlf]
  standard_name = latent_heat_of_fusion_of_water_at_0C
  long_name = latent heat of fusion
  units = J kg-1
  dimensions = ()
  type = real
  kind = kind_phys
  intent = in
[rvgas]
  standard_name = gas_constant_water_vapor
  long_name = ideal gas constant for water vapor
  units = J kg-1 K-1
  dimensions = ()
  type = real
  kind = kind_phys
  intent = in
[cp]
  standard_name = specific_heat_of_dry_air_at_constant_pressure
  long_name = specific heat of dry air at constant pressure
  units = J kg-1 K-1
  dimensions = ()
  type = real
  kind = kind_phys
  intent = in
[tin]
  standard_name = air_temperature_of_new_state
  long_name = temperature updated by physics
  units = K
  dimensions = (horizontal_loop_extent,vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
[qin]
  standard_name = specific_humidity_of_new_state
  long_name = water vapor specific humidity updated by physics
  units = kg kg-1
  dimensions = (horizontal_loop_extent,vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
[pfull]
  standard_name = air_pressure
  long_name = mean layer pressure
  units = Pa
  dimensions = (horizontal_loop_extent,vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
[cnvc]
  standard_name = convective_cloud_cover
  long_name = convective cloud cover
  units = frac
  dimensions = (horizontal_loop_extent,vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = in
[qs]
  standard_name = saturation_specific_humidity
  long_name = saturation specific humidity
  units = kg kg-1
  dimensions = (horizontal_loop_extent,vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = out
[qsl]
  standard_name = saturation_specific_humidity_with_respect_to_liquid
  long_name = saturation specific humidity with respect to liquid
  units = kg kg-1
  dimensions = (horizontal_loop_extent,vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = out
[qsi]
  standard_name = saturation_specific_humidity_with_respect_to_ice
  long_name = saturation specific humidity with respect to ice
  units = kg kg-1
  dimensions = (horizontal_loop_extent,vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = out
[dqsdT]
  standard_name = derivative_of_saturation_specific_humidity_with_respect_to_temperature
  long_name = derivative of saturation specific humidity with respect to temperature
  units = kg kg-1 K-1
  dimensions = (horizontal_loop_extent, vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = out
[U_ca]
  standard_name = relative_humidity_outside_convective_clouds
  long_name = relative humidity outside of convective cloud portion of grid box
  units = frac
  dimensions = (horizontal_loop_extent, vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = out
[U01]
  standard_name = maximum_nonconvective_cloud_area_fraction
  long_name = maximum portion of nonconvective grid box area that can be cloudy while maintaining grid-box-mean relative humidity
  units = frac
  dimensions = (horizontal_loop_extent, vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = out
[errmsg]
  standard_name = ccpp_error_message
  long_name = error message for error handling in CCPP
  units = none
  dimensions = ()
  type = character
  kind = len=*
  intent = out
[errflg]
  standard_name = ccpp_error_code
  long_name = error code for error handling in CCPP
  units = 1
  dimensions = ()
  type = integer
  intent = out
