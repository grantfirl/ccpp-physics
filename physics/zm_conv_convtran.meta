[ccpp-table-properties]
  name = zm_conv_convtran
  type = scheme
  dependencies =

########################################################################
[ccpp-arg-table]
  name = zm_conv_convtran_run
  type = scheme
[pcols]
  standard_name = horizontal_loop_extent
  long_name = horizontal loop extent
  units = count
  dimensions = ()
  type = integer
  intent = in
[pver]
  standard_name = vertical_layer_dimension
  long_name = number of vertical levels
  units = count
  dimensions = ()
  type = integer
  intent = in
[ixcldice]
  standard_name = index_of_cloud_ice_mixing_ratio_in_tracer_concentration_array
  long_name = index_of_cloud_ice_mixing_ratio_in_tracer_concentration_array
  units = index
  dimensions = ()
  type = integer
  intent = in
[ixcldliq]
  standard_name = index_of_cloud_liquid_water_mixing_ratio_in_tracer_concentration_array
  long_name = index_of_cloud_liquid_water_mixing_ratio_in_tracer_concentration_array
  units = index
  dimensions = ()
  type = integer
  intent = in
[cnst_type]
  standard_name = tracer_type
  long_name = type of mixing ratio (wet or dry)
  units = none
  dimensions = (number_of_tracers)
  type = character
  kind = len=3
  intent = in
[q]         
  standard_name = temporary_tracer_concentration
  long_name = temporary state model layer mean tracer concentration
  units = kg kg-1
  dimensions = (horizontal_loop_extent,vertical_layer_dimension,number_of_tracers)
  type = real
  kind = shr_kind_r8
  intent = in
[ncnst]
  standard_name = number_of_tracers
  long_name = number of tracers
  units = count
  dimensions = ()
  type = integer
  intent = in
[mu]
  standard_name = atmosphere_updraft_convective_mass_flux
  long_name = atmosphere updraft convective mass flux
  units = hPa s-1
  dimensions = (horizontal_loop_extent, vertical_layer_dimension)
  type = real
  kind = shr_kind_r8
  intent = in
[md]
  standard_name = atmosphere_downdraft_convective_mass_flux
  long_name = atmosphere downdraft convective mass flux
  units = hPa s-1
  dimensions = (horizontal_loop_extent, vertical_layer_dimension)
  type = real
  kind = shr_kind_r8
  intent = in
[du]
  standard_name = atmosphere_detrainment_convective_mass_flux_for_convective_triggered_columns
  long_name = atmosphere detrainment convective mass flux
  units = hPa s-1
  dimensions = (horizontal_loop_extent, vertical_layer_dimension)
  type = real
  kind = shr_kind_r8
  intent = in
[eu]
  standard_name = atmosphere_updraft_entrainment_convective_mass_flux
  long_name = atmosphere updraft entrainment convective mass flux
  units = hPa s-1
  dimensions = (horizontal_loop_extent, vertical_layer_dimension)
  type = real
  kind = shr_kind_r8
  intent = in
[ed]
  standard_name = atmosphere_downdraft_entrainment_convective_mass_flux
  long_name = atmosphere downdraft entrainment convective mass flux
  units = hPa s-1
  dimensions = (horizontal_loop_extent, vertical_layer_dimension)
  type = real
  kind = shr_kind_r8
  intent = in
[dp]
  standard_name = pressure_thickness_in_hPa
  long_name = model layer thickness in hPa
  units = hPa
  dimensions = (horizontal_loop_extent,vertical_layer_dimension)
  type = real
  kind = shr_kind_r8
  intent = in
[dsubcld]
  standard_name = subcloud_pressure_thickness_in_hPa
  long_name = thickness of subcloud layer in hPa
  units = hPa
  dimensions = (horizontal_loop_extent)
  type = real
  kind = shr_kind_r8
  intent = in
[jt]
  standard_name = vertical_index_at_cloud_top_for_convective_triggered_columns
  long_name = vertical index at cloud top for convective triggered columns
  units = index
  dimensions = (horizontal_loop_extent)
  type = integer
  intent = in
[mx]
  standard_name = vertical_index_at_maximum_moist_static_energy_for_convective_triggered_columns
  long_name = vertical index at maximum moist static energy for convective triggered columns
  units = index
  dimensions = (horizontal_loop_extent)
  type = integer
  intent = in
[ideep]
  standard_name = horizontal_index_of_convective_triggered_columns
  long_name = horizontal index of convective triggered columns
  units = index
  dimensions = (horizontal_loop_extent)
  type = integer
  intent = in
[il2g]
  standard_name = number_of_convective_triggered_columns
  long_name = number of convective triggered columns
  units = count
  dimensions = ()
  type = integer
  intent = in
[fracis]
  standard_name = fraction_of_tracer_that_is_insoluble
  long_name = fraction of transported species that are insoluble
  units = frac
  dimensions = (horizontal_loop_extent, vertical_layer_dimension, number_of_tracers)
  type = real
  kind = shr_kind_r8
  intent = in
[dqdt]
  standard_name = tendency_of_tracer_concentration_due_to_deep_convection_transport
  long_name = tendency of tracer concentration due to deep convection transport
  units = kg kg-1 s-1
  dimensions = (horizontal_loop_extent, vertical_layer_dimension, number_of_tracers)
  type = real
  kind = shr_kind_r8
  intent = inout
[errmsg]
  standard_name = ccpp_error_message
  long_name = error message for error handling in CCPP
  units = none
  dimensions = ()
  type = character
  kind = len=*
  intent = out
[errflg]
  standard_name = ccpp_error_code
  long_name = error flag for error handling in CCPP
  units = 1
  dimensions = ()
  type = integer
  intent = out
